# Your LiveKit server details
LIVEKIT_URL=
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=

# Agent Model Configuration
GOOGLE_API_KEY=
GROQ_API_KEY=
DEEPGRAM_API_KEY=
CARTESIA_API_KEY=

# Agent Room Configuration
AGENT_ROOM_PREFIX=agent_call
DEFAULT_AGENT_INSTRUCTIONS=You are a helpful AI assistant handling phone calls.

# Enable telephony features
ENABLE_TELEPHONY=1
# Twilio SIP Trunk IDs (you need both for inbound/outbound)
TWILIO_SIP_TRUNK_ID=your-twilio-sip-trunk-id
TWILIO_OUTBOUND_TRUNK_ID=your-twilio-outbound-trunk-id

# Optional: Twilio credentials (if needed for additional features)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token


# Webhooks
WEBHOOK_SERVER_PORT=
WEBHOOK_SERVER_HOST=
CALL_WEBHOOK_URL=


# Enable audio recording
ENABLE_EGRESS=1
EGRESS_USE_HLS=0   
# S3/R2 storage for recordings
S3_BUCKET=
S3_ACCESS_KEY=
S3_SECRET_KEY=
S3_ENDPOINT=
RECORDING_BASE_URL=
S3_REGION=
S3_FORCE_PATH_STYLE=
S3_FILEPATH=
EGRESS_SEGMENT_DURATION=

# Appointment Webhook Configuration
APPOINTMENT_WEBHOOK_URL=
# End Call Report Configuration
END_CALL_WEBHOOK_URL=
